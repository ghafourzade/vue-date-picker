    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "vue": "^3.2.47"
  },
  "dependencies": {
    "dayjs": "^1.11.7",
    "jalaliday": "^2.3.0"
