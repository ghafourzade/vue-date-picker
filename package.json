{
  "name": "@ghafourzade/date-picker",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/date-picker.umd.cjs",
  "module": "./dist/date-picker.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/date-picker.js",
      "require": "./dist/date-picker.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build && npm run build:types",
